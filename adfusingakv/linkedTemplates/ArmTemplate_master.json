{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfusingakv"
		},
		"synapsekeyvault-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapsekeyvault-WorkspaceDefaultSqlServer'"
		},
		"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "blobconnec"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://chestnut.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnec"
		},
		"linkedService_df_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnec"
		},
		"linkedService_df1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "blobconnec"
		},
		"linkedService_df2_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "blobconnec"
		},
		"synapsekeyvault-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stdatacoedata001.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfusingakv_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"synapsekeyvault-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('synapsekeyvault-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_connectionString_secretName')]"
					},
					"linkedService_df_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('linkedService_df_properties_typeProperties_connectionString_secretName')]"
					},
					"linkedService_df1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('linkedService_df1_properties_typeProperties_connectionString_secretName')]"
					},
					"linkedService_df2_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('linkedService_df2_properties_typeProperties_connectionString_secretName')]"
					},
					"synapsekeyvault-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('synapsekeyvault-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfusingakv_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfusingakv_ArmTemplate_0')]"
			]
		}
	]
}